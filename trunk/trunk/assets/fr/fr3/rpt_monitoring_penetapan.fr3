<TfrxReport Version="4.9.95" DotMatrixReport="False" EngineOptions.UseFileCache="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40331,8440824653" ReportOptions.Description.Text="" ReportOptions.LastChange="40407,93655" ScriptLanguage="PascalScript" ScriptText.Text="&#13;&#10;begin&#13;&#10;    if &#60;jenis&#62;='ALL' then begin&#13;&#10;       page1.visible:=true;&#13;&#10;       page2.visible:=false;&#13;&#10;       qrydaftarpenetapan.sql.text:=&#13;&#10;               'select a.TGL_PENETAPAN, a.NO_PENETAPAN, a.NOMINAL_PENETAPAN, a.TGL_SETOR, vdata.NPWP, vdata.PEMOHON, vdata.ALAMAT, vdata.PERIODE_AWAL, vdata.PERIODE_AKHIR,rk.KODE_REKENING,rk.NAMA_REKENING '+&#13;&#10;               'from penetapan_pr a '+&#13;&#10;               'join penetapan_pr_content aa on aa.PENETAPAN_PR_ID=a.PENETAPAN_PR_ID '+&#13;&#10;               'join penetapan_pr_detail ab on ab.PENETAPAN_PR_ID=a.PENETAPAN_PR_ID '+                       &#13;&#10;               'join v_pendataan_spt vdata on vdata.PENDATAAN_ID=aa.PENDATAAN_ID '+&#13;&#10;               'join rekening_kode rk on rk.ID=ab.ID_REKENING '+      &#13;&#10;               'where a.TGL_PENETAPAN between '''+FormatDateTime('yyyy-mm-dd',strtodate(&#60;TanggalAwal&#62;))+''' and '''+&#13;&#10;               FormatDateTime('yyyy-mm-dd',strtodate(&#60;TanggalAkhir&#62;))+'''';&#13;&#10;                                  &#13;&#10;    end else begin&#13;&#10;       page1.visible:=false;&#13;&#10;       page2.visible:=true;&#13;&#10;         &#13;&#10;       {qrydaftarpenetapan.sql.text:=&#13;&#10;               'select a.TGL_PENETAPAN, a.NO_PENETAPAN, a.NOMINAL_PENETAPAN, a.TGL_SETOR, vdata.NPWP, vdata.PEMOHON, vdata.ALAMAT, vdata.PERIODE_AWAL, vdata.PERIODE_AKHIR,rk.KODE_REKENING,rk.NAMA_REKENING '+&#13;&#10;               'from penetapan_pr a '+&#13;&#10;               'join penetapan_pr_content aa on aa.PENETAPAN_PR_ID=a.PENETAPAN_PR_ID '+&#13;&#10;               'join v_pendataan_spt vdata on vdata.PENDATAAN_ID=aa.PENDATAAN_ID '+&#13;&#10;               'join rekening_kode rk on rk.ID=ab.ID_REKENING '+                       &#13;&#10;               'where a.TGL_PENETAPAN between '''+FormatDateTime('yyyy-mm-dd',strtodate(&#60;TanggalAwal&#62;))+''' and '''+&#13;&#10;               FormatDateTime('yyyy-mm-dd',strtodate(&#60;TanggalAkhir&#62;))+''' and rk.id='''+IntToStr(&#60;rekening&#62;)+'''';  &#13;&#10;        }                         &#13;&#10;    end;              &#13;&#10;end." PropData="">
